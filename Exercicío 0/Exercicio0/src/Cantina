public class Cantina {

        String nome;
        Salgado [] salgados = new Salgado[3];
    
        void addSalgado(Salgado novoSalgado){
    
            for (int i=0; i<salgados.lenght;i++){
                salgados [i] = novoSalgado;
            }
    
        }
    
        void mostraInfo(){
            for(Salgado salgado:salgados){
                System.out.println(salgado);
    
            }
            
        }
    }
